{
  "name": "H7",
  "title": "H7",
  "description": "ESP32开发平台",
  "url": "https://espressif.com/",
  "homepage": "https://github.com/OS-Q/H7",
  "license": " ",
  "engines": {
    "platformio": "<5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/H7.git"
  },
  "version": "1.0.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json",
    {
	    "framework-N16": {
		"type": "framework",
		"version": "https://github.com/OS-Q/N16.git"
	    },
	    "framework-N17": {
		"type": "framework",
		"version": "https://github.com/OS-Q/N17.git"
	    },
   }
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-N15",
      "script": "builder/frameworks/arduino.py"
    },
    "espidf": {
      "package": "framework-N14",
      "script": "builder/frameworks/espidf.py"
    },
    "simba": {
      "package": "framework-N16",
      "script": "builder/frameworks/simba.py"
    },
    "pumbaa": {
      "package": "framework-N17",
      "script": "builder/frameworks/pumbaa.py"
    }
  },
  "packages": {
    "framework-N15": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N15.git"
    },
    "framework-N14": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/framework-N14.git"
    },
    "framework-N16": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/framework-N16.git"
    },
    "framework-N17": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/framework-N17.git"
    },
    "toolchain-xtensa32": {
      "type": "toolchain",
      "version": "~2.50200.0"
    },
    "toolchain-esp32ulp": {
      "type": "toolchain",
      "optional": true,
      "version": "~1.22851.190618"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "version": "~1.20600.0"
    },
    "tool-openocd-esp32": {
      "type": "debugger",
      "optional": true,
      "version": "~1.1000.20181026"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "version": "~2.230.0"
    }
  }
}
